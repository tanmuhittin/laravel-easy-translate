{
  "name": "tanmuhittin/laravel-easy-translate",
  "description": "A package for database translations.",
  "authors": [
    {
      "name": "Muhittin Tan",
      "email": "tanmuhittin@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {},

  "autoload": {
    "psr-4": {
      "TanMuhittin\\LaraTranslate\\": "src/"
    }
  },

  "extra": {
    "laravel": {
      "providers": [
        "TanMuhittin\\LaraTranslate\\LaraTranslateServiceProvider"
      ],

      "aliases": {
        "Translatable": "TanMuhittin\\LaraTranslate\\Facades\\Translatable"
      }
    }
  }

}